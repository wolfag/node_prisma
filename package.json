  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.4",
    "@hapi/hapi": "^20.2.1",
    "@hapi/joi": "^17.1.1",
    "@prisma/client": "3.11.0",
    "date-fns": "^2.28.0"
  },
  "devDependencies": {
    "@types/hapi__hapi": "^20.0.10",
    "@types/hapi__joi": "^17.1.8",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  }
}
