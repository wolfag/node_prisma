{
  "name": "prisma-class",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn ./src/server.ts",
    "seed": "ts-node ./src/seed.ts",
    "test": "jest",
    "compile": "tsc",
    "postgres:start": "docker-compose up -d",
    "migrate:save": "prisma migrate --experimental save",
